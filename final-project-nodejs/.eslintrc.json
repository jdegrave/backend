{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "no-prototype-builtins" : 0,
        "prefer-rest-params" : 0,
        "no-unneeded-ternary" : 0,
        "array-callback-return" : 0,
        "no-useless-concat" : 0,
        "consistent-return" : 0,
        "no-multi-assign" : 0,
        "no-consistent-return" : 0,
        "no-underscore-dangle" : 0,
        "no-shadow" : 0,
        "no-else-return" : 0,
        "max-len": 0,
        "object-property-newline": 0,
        "array-bracket-spacing" : 0,
        "no-param-reassign" : 0,
        "no-console": 0,
        "key-spacing": 0,
        "spaced-comment" : 0,
        "no-path-concat" : 0,
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "indent": ["error", 4],
        "no-var": 0,
        "comma-dangle": ["error", "never"],
        "prefer-arrow-callback": 0,
        "func-names": 0,
        "prefer-template": 0,
        "object-shorthand": 0,
        "no-use-before-define": 0,
        "space-before-function-paren": ["error", {
            "anonymous": "always",
            "named": "always",
            "asyncArrow": "ignore"
        }],
        "vars-on-top": 0,
        "import/newline-after-import": 0,
        "no-plusplus" : 0,
        "no-mixed-operators" : 0,
        "no-lonely-if" : 0,
        "padded-blocks" : 0,
        "no-return-assign" : 0
    }
}
